{
  "name": "so-challenge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "heroku-cleanup": "npm run migrate:prod",
    "lint": "eslint \"{database,src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "migration:generate": "npm run typeorm -- migration:create -c migration -n $1",
    "migrate:dev": "npm run typeorm -- migration:run -c migration-dev",
    "migrate:dev:down": "npm run typeorm -- migration:revert -c migration-dev",
    "migrate:prod": "npm run typeorm -- migration:run -c migration-prod",
    "migrate:prod:down": "npm run typeorm -- migration:revert -c migration-prod",
    "seed:generate": "npm run typeorm -- migration:create -c seed -n $1",
    "seed": "npm run typeorm -- migration:run -c seed",
    "seed:down": "npm run typeorm -- migration:revert -c seed",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.3",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.4.4",
    "@nestjs/platform-express": "^7.0.3",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.0.0",
    "@sentry/node": "^5.15.5",
    "axios": "^0.19.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.13.0",
    "express-handlebars": "^4.0.4",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "morgan": "^1.9.1",
    "pg": "^8.0.3",
    "pm2": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.3",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.24",
    "typeorm-transactional-cls-hooked": "^0.1.12",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.3",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.5.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
